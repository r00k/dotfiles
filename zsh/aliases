alias h="heroku"
alias less="less -R"
alias ls="ls -la"
alias reload_aliases="source ~/.zsh/aliases"
alias v='vim'
alias g="hub"
alias be="bundle exec"
alias undle="bundle"
alias befs="foreman start bundle exec"
alias befr="foreman run bundle exec"

# Git aliases
alias gcom='git checkout master'
alias gplom='git pull origin master'

